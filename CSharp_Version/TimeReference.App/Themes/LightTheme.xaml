<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackground" Color="#F3F3F3"/>
    <SolidColorBrush x:Key="CardBackground" Color="White"/>
    <SolidColorBrush x:Key="PrimaryText" Color="#444444"/>
    <SolidColorBrush x:Key="SecondaryText" Color="Gray"/>
    <SolidColorBrush x:Key="AccentColor" Color="#0078D4"/>
    <SolidColorBrush x:Key="SuccessColor" Color="#28A745"/>
    <SolidColorBrush x:Key="WarningColor" Color="#D4A017"/>
    <SolidColorBrush x:Key="ErrorColor" Color="Red"/>
    <SolidColorBrush x:Key="BorderColor" Color="#DDDDDD"/>
    <SolidColorBrush x:Key="ConsoleBackground" Color="#F8F8F8"/>
    <SolidColorBrush x:Key="ConsoleForeground" Color="#444444"/>
    <Color x:Key="ShadowColor">Black</Color>
</ResourceDictionary>
