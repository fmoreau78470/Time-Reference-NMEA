<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackground" Color="#121212"/>
    <SolidColorBrush x:Key="CardBackground" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="PrimaryText" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="SecondaryText" Color="#A0A0A0"/>
    <SolidColorBrush x:Key="AccentColor" Color="#4CC2FF"/>
    <SolidColorBrush x:Key="SuccessColor" Color="#6CC26C"/>
    <SolidColorBrush x:Key="WarningColor" Color="#FFD700"/>
    <SolidColorBrush x:Key="ErrorColor" Color="#FF6B6B"/>
    <SolidColorBrush x:Key="BorderColor" Color="#333333"/>
    <SolidColorBrush x:Key="ConsoleBackground" Color="Black"/>
    <SolidColorBrush x:Key="ConsoleForeground" Color="#CCCCCC"/>
    <Color x:Key="ShadowColor">Black</Color>
</ResourceDictionary>
